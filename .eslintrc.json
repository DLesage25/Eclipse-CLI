{
    "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "ignorePatterns": ["lib", "bin", "node_modules"],
    "env": {
        "node": true
    },
    "root": true,
    "rules": {
        "@typescript-eslint/ban-ts-comment": 1
    }
}
