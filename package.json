{
    "name": "@eclipse/cli",
    "version": "0.2.1",
    "description": "Inject environment variables on runtime",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "repository": "https://github.com/dlesage25/eclipse-cli.git",
    "author": "Daniel Lesage @dlesage25",
    "license": "MIT",
    "scripts": {
        "start": "npm run build && npm run global",
        "build": " rm -rf ./lib && npx rollup -c",
        "global": "sudo npm i -g && eclipse",
        "reset": "rm -rf ./node_modules ./package-lock.json ./lib && npm install",
        "lint": "npx eslint ."
    },
    "typeCoverage": {
        "minCoverage": 90
    },
    "bin": {
        "eclipse": "bin/eclipse"
    },
    "keywords": [
        "cli",
        "liftr",
        "eclipse",
        "typescript",
        "javascript",
        "type",
        "coverage",
        "nodejs"
    ],
    "dependencies": {
        "axios": "^0.27.2",
        "color-support": "^1.1.3",
        "commander": "^2.19.0",
        "dotenv": "^16.0.2",
        "figlet": "^1.2.1",
        "glob": "7.1.3",
        "inquirer": "^8.2.4",
        "inversify": "^5.0.1",
        "keytar": "^7.9.0",
        "minimist": "1.2.0",
        "open": "^8.4.0",
        "prompts": "^2.0.1",
        "reflect-metadata": "^0.1.13",
        "tslib": "^2.4.0"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^22.0.2",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^14.0.0",
        "@rollup/plugin-replace": "^5.0.0",
        "@rollup/plugin-typescript": "^8.5.0",
        "@types/axios": "^0.14.0",
        "@types/color-support": "^1.1.1",
        "@types/figlet": "^1.5.5",
        "@types/glob": "7.1.1",
        "@types/inquirer": "^9.0.1",
        "@types/jasmine": "3.3.7",
        "@types/minimist": "1.2.0",
        "@types/node": "10.12.18",
        "@typescript-eslint/eslint-plugin": "^5.36.2",
        "@typescript-eslint/parser": "^5.36.2",
        "eslint": "^8.23.0",
        "jasmine": "3.3.1",
        "no-unused-export": "1.7.0",
        "rollup": "^2.79.0",
        "rollup-plugin-auto-external": "^2.0.0",
        "rollup-plugin-node-globals": "^1.4.0",
        "rollup-plugin-terser": "^7.0.2",
        "standard": "12.0.1",
        "ts-node": "^7.0.1",
        "typescript": "^3.9.10"
    }
